  .h-container {
    max-width: 1440px;
    width: 90%;
    margin: 0 auto;
  }  

  .h-banner {
    position: relative;
    overflow: hidden;
    height: 80vh;
    max-height: 800px;
  }

  .picture-banner > img,
  .h-banner .slide-video {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .h-banner .caption {
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    text-align: right;
    color: #fff;
    font-size: 14px;
    line-height: 1;
  }

  .h-banner h2 {
    font-size: 36px;
    line-height: 1;
    margin: 20px 0;
    text-transform: uppercase;
  }

  .h-banner p {
    margin: 0 0 20px;
    font-family: arial;
  }

  .h-banner .button {
    background-color: #000;
