    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=5.0">
    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
    <link rel="preconnect" href="https://fonts.shopify.com" crossorigin>
    <link rel="preconnect" href="https://monorail-edge.shopifysvc.com">

    <link rel="preload" href="{{ 'theme--critical.css' | asset_url }}" as="style">
    <link rel="preload" href="{{ 'theme--async.css' | asset_url }}" as="style">
    	<link href="https://blogfonts.com/css/aWQ9MTgzMDA3JnN1Yj03JmM9bCZ0dGY9bHVpbXAudHRmJm49bHVpbXA/Luimp.ttf" rel="stylesheet" type="text/css"/>

    <link
      rel="stylesheet"
      href="https://unpkg.com/swiper/swiper-bundle.min.css"
    />

    {%- if canonical_url != blank  -%}
      <link rel="canonical" href="{{ canonical_url }}">
    {%- endif -%}

    {% if settings.favicon != blank %}
      <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x' }}" type="image/png">
      <link rel="apple-touch-icon" href="{{ settings.favicon | img_url: '180x' }}">
    {% endif %}
    {% render 'framework--social-meta-tags' %}

    {% if template == 'index' %}
      <title>{{ page_title }}</title>
    {% elsif template == 'password' %}
      <title>{{ shop.name }}</title>
    {% else %}
      <title>{{ page_title }} | {{ shop.name }}</title>
    {% endif %}

    {% render 'framework--css-variables' %}
    {% render 'partial--css-variables' %}
    {% render 'partial--js-variables' %}

    <script>
      function onLoadStylesheet() {
        var link = document.querySelector('link[href="' + theme.assets.stylesheet + '"]');
        link.loaded = true;
        link.dispatchEvent(new Event('load'));
      }
    </script>

    <link href="{{ 'theme--critical.css' | asset_url }}" rel="stylesheet" media="all"/>
    <link
      href="{{ 'theme--async.css' | asset_url }}"
      rel="stylesheet"
      media="print"
      onload="this.media='all';onLoadStylesheet()"
    >

    <!-- Add the slick-theme.css if you want default styling -->
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css">

    <style>{% render 'custom-styles' %}</style>
    
   

    <script src="{{ 'lazysizes.min.js' | asset_url }}" async="async"></script>
    <script src="{{ 'theme.min.js' | asset_url }}" async="async"></script>

    {% if template contains 'customers' %}
      <script src="{{ 'shopify_common.js' | shopify_asset_url }}" defer="defer"></script>
      <script src="{{ 'customer_area.js' | shopify_asset_url }}" defer="defer"></script>
    {% endif %}
    
  <!-- Yotpo's JavaScript -->
  {% comment %} Yotpo content removed from here {% endcomment %}
    {{ content_for_header }} 
 {{ shop.metafields.fontify.stylesheets }}
    
    <script src="https://cdn.attn.tv/thebrowtrio/dtag.js"></script>
  
  {% render 'shogun-head' %}
  {% include 'qikify-smartbar' %}
   

  {% include 'friendbuy' %}
</head>

  <body
    class="page-{{ page_title | handle }} template-{{ template | split: '.' | first | remove: '/' | remove: '_' }}"
    data-theme-id="606"
    data-theme-name="Blockshop"
    data-theme-version="6.8.0"
  >
    {% unless template contains 'password' %}
      <a data-item="a11y-button" href='#main-content'>
        {{ 'general.skip_link' | t }}
      </a>
    {% endunless %}

    <div
      class="off-canvas--viewport"
      data-js-class="OffCanvas"
      data-off-canvas--state="closed"
    ><div hidden id="1e7c9b96eab397d0921eef5a5a0b83e05e1d7a10"></div> 
      <div class="off-canvas--overlay"></div>
      <div class="off-canvas--close" aria-label="{{ 'general.close' | t }}">
        {% render 'framework--icons', icon: 'cross' %}
      </div>

      <div class="off-canvas--left-sidebar">
        <div class="off-canvas--focus-trigger" tabindex="0"></div>
        {% render 'partial--mobile-nav' %}
        <div class="off-canvas--focus-trigger" tabindex="0"></div>
      </div>

      <div class="off-canvas--right-sidebar" data-active="cart">
        <div class="off-canvas--focus-trigger" tabindex="0"></div>
        <div class="off-canvas--container" data-view="pickup"></div>

        {% if settings.cart--type == 'drawer' %}
          <div class="off-canvas--container" data-view="cart">
            {% render 'framework--cart', view: 'mobile' %}
          </div>
        {% endif %}

        <div class="off-canvas--focus-trigger" tabindex="0"></div>
      </div>

      <div class="off-canvas--main-content">
        {% section 'announcement' %}
        {% section 'header' %}

        <section class="layout--main-content" id="main-content">
          {{ content_for_layout }}
        </section>
        {% section 'footer' %}
      

        {% unless template == 'password' %}
          <div class="popup-modal-close" aria-label="{{ 'general.close' | t }}">
            {% render 'framework--icons', icon: 'cross' %}
          </div>
          <div class="popup-modal-mask"></div>
        {% endunless %}
      </div>

      {% render 'framework--feedback-bar' %}
      {% render 'partial--popup' %}

    </div>
{% render 'afterpay' %}
    {% render 'framework--search', view: 'modal' %}
    {% render 'framework--modal' %}

  
    <!-- Swiper JS -->
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>

    <!-- Initialize Swiper -->
    <script>
      var swiper = new Swiper(".mySwiper", {
        slidesPerView: 2,
        spaceBetween: 10,
        breakpoints: {
          768: {
            slidesPerView: 3,
            spaceBetween: 40
          }
        },
        pagination: {
          el: ".swiper-pagination",
          clickable: true
        },
        navigation: {
          nextEl: ".swiper-button-next",
          prevEl: ".swiper-button-prev",
        },

      });

      var swiper = new Swiper(".mySwiper2", {
        slidesPerView: 1,
        spaceBetween: 0,
        breakpoints: {
          768: {
            slidesPerView: 5,
            spaceBetween: 10
          }
        },
        navigation: {
          nextEl: ".swiper-button-next",
          prevEl: ".swiper-button-prev",
        }

      });
    </script>

<!-- Back in Stock helper snippet -->
{% render 'back-in-stock-helper' %}
