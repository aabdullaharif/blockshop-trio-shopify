    <script>
      $('.tr_slick_main').slick({
        slidesToShow: 1,
        slidesToScroll: 1,
        arrows: false,
        fade: true,
        dots: true,
        adaptiveHeight: true,
        mobileFirst: true,
        asNavFor: '.tr_slick_thumb',
        responsive: [{
          breakpoint: 991,
          settings: {
            dots: false
          }
        }, {
          breakpoint: 480,
          settings: {
            dots: true
          }
        }]
      });
      $('.tr_slick_thumb').slick({
        slidesToShow: 5,
        slidesToScroll: 1,
        asNavFor: '.tr_slick_main',
        dots: false,
        arrows: false,
        infinite: true,
        centerMode: false,
        mobileFirst: true,
        focusOnSelect: true,
        verticalSwiping: false,
        responsive: [{
          breakpoint: 991,
          settings: {
            vertical: true,
            slidesToShow: 5,
            slidesToScroll: 1
          }
        }, {
          breakpoint: 480,
          settings: {
            slidesToShow: 3,
            slidesToScroll: 1
          }
        }]
      });

      $('.product-carousel').slick({
        speed: 300,
        dots: false,
        autoplay: true,
        draggable: false,
        swipeToSlide: false,
        touchMove: false,
        infinite: true,
        slidesToShow: 2,
        slidesToScroll: 1,
        mobileFirst: true,
        responsive: [
          {
            breakpoint: 1024,
            settings: {
              slidesToShow: 4,
              slidesToScroll: 1
            }
          },
          {
            breakpoint: 600,
            settings: {
              slidesToShow: 3,
              slidesToScroll: 1
            }
          },
          {
            breakpoint: 480,
            settings: {
              slidesToShow: 2,
              slidesToScroll: 1
            }
          }
        ]
      });

      $('.search-result-carousel').slick({
        speed: 300,
        dots: false,
        arrows: true,
        draggable: false,
        swipeToSlide: false,
        touchMove: false,
        infinite: false,
        slidesToShow: 2,
        slidesToScroll: 1,
        mobileFirst: true,
        responsive: [
          {
            breakpoint: 1024,
            settings: {
              slidesToShow: 4,
              slidesToScroll: 1
            }
          },
          {
            breakpoint: 600,
            settings: {
              slidesToShow: 3,
              slidesToScroll: 1
            }
          },
          {
            breakpoint: 480,
            settings: {
              slidesToShow: 2,
              slidesToScroll: 1
            }
          }
        ]
      }).on('setPosition', function (event, slick) {
        slick.$slides.css('height', slick.$slideTrack.height() + 'px');
      });

      $(function() {
        //----- OPEN
        $('[data-popup-open]').on('click', function(e) {
          var targeted_popup_class = jQuery(this).attr('data-popup-open');
          $('[data-popup="' + targeted_popup_class + '"]').fadeIn(350);

          e.preventDefault();
        });

        //----- CLOSE
        $('[data-popup-close]').on('click', function(e) {
          var targeted_popup_class = jQuery(this).attr('data-popup-close');
          $('[data-popup="' + targeted_popup_class + '"]').fadeOut(350);

          e.preventDefault();
        });
      });


      $('.big-box').slick({
        slidesToShow: 1,
        slidesToScroll: 1,
        arrows: false,
        fade: true,
        asNavFor: '.items-thumbs'
      });

      $('.items-thumbs').on('mouseover', function(e) {
        "use strict";
        var mouseSide;
        if ((e.pageX - this.offsetLeft) < $(this).width() / 2) {
          $('.items-thumbs').slick('slickPrev');
        } else {
          $('.items-thumbs').slick('slickNext');
        }
      });

      $('.items-thumbs').slick({
        slidesToShow: 5,
        slidesToScroll: 1,
        asNavFor: '.big-box',
        dots: false,
        arrows: false,
        focusOnSelect: true
      });

      $('.tabs .tab-links a').on('click', function(e) {
        var currentAttrValue = $(this).attr('href');
        $('.tabs ' + currentAttrValue).show().siblings().hide();
        $(this).parent('li').addClass('active').siblings().removeClass('active');
        e.preventDefault();
      });

      $('.main-frame .prod-button').on('click', function(e) {
        var currentAttrValue = $(this).attr('href');
        $('.main-frame ' + currentAttrValue).show().siblings().hide();
        $('.main-frame .prod-button.active').removeClass('active');
        $(this).addClass('active');
        e.preventDefault();
      });

      $('.btn-close').on('click', function() {
        $('.main-frame .prod-button.active').removeClass('active');
        $('.main-frame .slider-tab').hide();
      });

      $('.main-frame .prod-button').on('click', function() {
        $('.slider-tab .slick-slider').resize();
        $('.slider-tab .slick-slider').slick('refresh');
      });

      $(function() {
        function close_accordion_section() {
          $('.accordion .accordion-section-title').removeClass('active');
          $('.accordion .accordion-section-content').slideUp(300).removeClass('open');
        }
        $('.accordion-section-title').click(function(e) {
          var currentAttrValue = $(this).attr('href');
          if($(e.target).is('.active')) {
            close_accordion_section();
          }else {
            close_accordion_section();
            $(this).addClass('active');
            $('.accordion ' + currentAttrValue).slideDown(300).addClass('open'); 
          }
          e.preventDefault();
        });
      });
      //  $('input[type=radio][name=option-0]').change(function() {
