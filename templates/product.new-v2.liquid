  var _learnq = _learnq || [];

  var item = {
    Name: {{ product.title|json }},
    ProductID: {{ product.id|json }},
    Categories: {{ product.collections|map:'title'|json }},
    ImageURL: "https:{{ product.featured_image.src|img_url:'grande' }}",
    URL: "{{ shop.secure_url }}{{ product.url }}",
    Brand: {{ product.vendor|json }},
    Price: {{ product.price|money|json }},
    CompareAtPrice: {{ product.compare_at_price_max|money|json }}
  };

  _learnq.push(['track', 'Viewed Product', item]);
  _learnq.push(['trackViewedItem', {
    Title: item.Name,
    ItemId: item.ProductID,
    Categories: item.Categories,
    ImageUrl: item.ImageURL,
    Url: item.URL,
    Metadata: {
      Brand: item.Brand,
      Price: item.Price,
      CompareAtPrice: item.CompareAtPrice
    }
  }]);
</script>


<script>
  $(function(){
    var stickyHeaderTop = $('#stickytypeheader').offset().top;

    $(window).scroll(function(){
      if( $(window).scrollTop() > stickyHeaderTop ) {
        $('#stickytypeheader').css({position: 'fixed', top: '22px'});
        $('#stickytypeheader').css('display', 'block');
        $('#stickytypeheader').css('opacity', '1');
        $('#staticheader').css('display', 'none');
      } else {
        $('#stickytypeheader').css({position: 'static', top: '0px'});
        $('#stickytypeheader').css('display', 'none');
        $('#staticheader').css('display', 'flex');
      }
    });
  });
</script>







